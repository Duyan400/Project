package agile;


import org.junit.After;
import org.junit.AfterClass;
import static org.junit.Assert.*;
import org.junit.Before;
import org.junit.BeforeClass;
import org.junit.Test;

/**
 *
 * @author AmineZ
 */
public class LesGettersTest {
    
    public LesGettersTest() {
    }
    
    @BeforeClass
    public static void setUpClass() {
    }
    
    @AfterClass
    public static void tearDownClass() {
    }
    
    @Before
    public void setUp() {
    }
    
    @After
    public void tearDown() {
    }

    @Test
    public void testGetDossier() throws Exception {
       
      String resultatAttendu = "C160323";
       assertTrue(resultatAttendu.equals("C160323"));
       
    }
    

    @Test
    public void testGetNumeroClient() throws Exception {
     String resultatAttendu = "160323";
     assertTrue(resultatAttendu.equals("160323"));
  
    }

    @Test
    public void testGetContrat() throws Exception {
         String resultatAttendu = "C";
        assertTrue(resultatAttendu.equals("C"));
    }

    @Test
    public void testGetMois() throws Exception {
    String resultatAttendu = "2013-03";
    assertTrue(resultatAttendu.equals("2013-03"));
    }

    @Test
    public void testGetSoins() throws Exception {
       String resultatAttendu= "175";
       assertTrue(resultatAttendu.equals("175"));
    }

    @Test
    public void testGetCodes() throws Exception {
       String resultatAttendu= "A";
       assertTrue(resultatAttendu.equals("A"));
    }

    @Test
    public void testGetDates() throws Exception {
   String resultatAttendu= "2013-03-02";
       assertTrue(resultatAttendu.equals("2013-03-02"));
    }

    @Test
    public void testGetMontants() throws Exception {
     String resultatAttendu= "100,00";
       assertTrue(resultatAttendu.equals("100,00"));
    }
}
